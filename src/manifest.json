{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Stackovershits",
  "description": "Google search cleaner for shitty stackoverflow clones",
  "author": "Yarfuo <yarfuo@gmail.com>",
  "permissions": [
    "https://www.google.com/search?*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["block.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
